{
  "manifest_version": 3,
  "name": "Webpage Editor",
  "version": "1.0",
  "description": "Edit elements on any webpage",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": ["https://open.spotify.com/*"]
    }
  ]
}