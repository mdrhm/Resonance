{
  "manifest_version": 3,
  "name": "Resonance",
  "icons": {

    "16": "logo.png",

    "32": "logo.png",

    "48": "logo.png",

    "128": "logo.png"

  },
  "version": "1.1",
  "description": "Rate your favorite Spotify tracks effortlessly",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "matches": ["https://open.spotify.com/*"]
    }
  ]
}