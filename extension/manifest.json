{
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "resonance@resonance.com",
      "strict_min_version": "58.0"
    }
  },
  "name": "Resonance",
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://resonanceapi.pythonanywhere.com/*"],
  "version": "2.0",
  "description": "Rate your favorite Spotify tracks effortlessly",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js",
        "user.js"
      ],
      "css": [
        "style.css",
        "user.css"
      ],
      "matches": [
        "https://open.spotify.com/*"
      ]
    }
  ]
}